{
  "name": "mawoscha/german-localepack-es-es",
  "type": "magento-module",
  "license":"OSL-3.0",
  "description":"Spanish (Spain) Community Translation",
  "authors":[
    {
      "name":"MaWoScha"
    }
  ],
  "require": {
    "magento-hackathon/magento-composer-installer": "*"
  },
  "extra": {
    "magento-root-dir": "./",
    "magento-deploystrategy": "copy",
    "map": [
      ["./app/code/community/German/LocalePackEs", "./app/code/community/German/LocalePackEs"],
      ["./app/etc/modules/German_LocalePackEs.xml", "./app/etc/modules/German_LocalePackEs.xml"],
      ["./downloader/template_es_1.5", "./downloader/template_es_1.5"],
      ["./downloader/template_es_1.7", "./downloader/template_es_1.7"],
      ["./app/locale/de_DE/German_LocaleEs.csv", "./app/locale/de_DE/German_LocaleEs.csv"],
      ["./app/locale/en_US/German_LocaleEs.csv", "./app/locale/en_US/German_LocaleEs.csv"],
      ["./app/locale/es_ES/German_LocaleEs.csv", "./app/locale/es_ES/German_LocaleEs.csv"],
      ["./app/locale/it_IT/German_LocaleEs.csv", "./app/locale/it_IT/German_LocaleEs.csv"],
      ["./app/locale/fr_FR/German_LocaleEs.csv", "./app/locale/fr_FR/German_LocaleEs.csv"],
      ["./app/locale/pt_PT/German_LocaleEs.csv", "./app/locale/pt_PT/German_LocaleEs.csv"]
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
